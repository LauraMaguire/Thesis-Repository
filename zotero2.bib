@article{wu11,
  title = {The 1.9~{{\AA}} Crystal Structure of {{Prp20p}} from {{Saccharomyces}} Cerevisiae and Its Binding Properties to {{Gsp1p}} and Histones},
  volume = {174},
  issn = {1047-8477},
