  file = {/Users/lauramaguire/Google Drive/papers/Mishra et al. - 2019 - The Effect of FG-Nup Phosphorylation on NPC Select.pdf;/Users/lauramaguire/Zotero/storage/RRHGEA6N/htm.html}
}

