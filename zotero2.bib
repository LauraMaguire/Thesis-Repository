  file = {/Users/lauramaguire/Google Drive/papers/Schleicher et al. - 2014 - Selective transport control on molecular velcro ma.pdf;/Users/lauramaguire/Zotero/storage/9AEIZVXC/nnano.2014.103.html}
}

