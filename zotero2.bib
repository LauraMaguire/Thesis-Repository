@article{obrien06,
  title = {Oxygen Inhibition in Thiol\textendash{}Acrylate Photopolymerizations},
  volume = {44},
  issn = {1099-0518},
  doi = {10.1002/pola.21304},
  abstract = {The overall effects of oxygen on thiol\textendash{}acrylate photopolymerizations were characterized. Specially, the choice of thiol monomer chemistry, functionality, and concentration on the extent of oxygen inhibition were considered. As thiol concentration was increased, the degree of oxygen inhibition was greatly reduced because of chain transfer from the peroxy radical to the thiol. When comparing the copolymerization of 1,6-hexanediol diacrylate with the alkane-based thiol (1,6-hexane dithiol) to the copolymerization with the propionate thiol (glycol dimercaptopropionate), it was found that the propionate system was much more reactive and polymerized to a greater extent in the presence of oxygen. In addition, the functionality was considered where the glycol dimercaptopropionate was compared to a tetrafunctional propionate of similar chemistry (pentaerythritol tetrakis(mercaptopropionate)). Given the same thiol concentration, the higher functionality thiol imparted a faster polymerization rate, due to the increased polymer system viscosity, which limited oxygen diffusion and decreased the extent of overall oxygen inhibition. Thus, preliminary insight is provided into how thiol monomer choice affects the extent of oxygen inhibition in thiol\textendash{}acrylate photopolymerization. \textcopyright{} 2006 Wiley Periodicals, Inc. J Polym Sci Part A: Polym Chem 44: 2007\textendash{}2014, 2006},
  language = {en},
  number = {6},
  journal = {Journal of Polymer Science Part A: Polymer Chemistry},
  author = {O'Brien, Allison K. and Cramer, Neil B. and Bowman, Christopher N.},
  month = mar,
  year = {2006},
  keywords = {curing of polymers,kinetics,oxygen inhibition photopolymerization,thiol–ene,photoinhibitor},
  pages = {2007-2014},
  file = {C:\\Users\\Laura\\Documents\\Papers\\O'Brien et al_2006_Oxygen inhibition in thiolâacrylate photopolymerizations.pdf;/Users/lauramaguire/Zotero/storage/HQV322UR/O'Brien et al. - 2006 - Oxygen inhibition in thiol–acrylate photopolymeriz.html}
