  keywords = {avidity,intrinsically disordered protein,nuclear transport,nuclear magnetic resonance (NMR),transport,nuclear pore complex,nuclear pore,FG Nups/FG repeats,isothermal titration calorimetry (ITC),multifunctional protein,multivalent interactions/fuzzy interactions,nuclear transport/NTF2,Phe-Gly nucleoporin,protein complex,transport factor},
  pages = {4555-4563},
  file = {/Users/lauramaguire/Google Drive/papers/Hayama et al. - 2018 - Thermodynamic characterization of the multivalent .pdf;/Users/lauramaguire/Google Drive/papers/Hayama et al. - 2018 - Thermodynamic characterization of the multivalent 2.pdf;/Users/lauramaguire/Zotero/storage/GYZDUNNB/jbc.AC117.001649.full.pdf;/Users/lauramaguire/Zotero/storage/N5WL5VEL/jbc.AC117.001649.html;/Users/lauramaguire/Zotero/storage/ZN57AZDJ/4555.html},
