@article{chon14,
  title = {Single Molecule Confocal Fluorescence Lifetime Correlation Spectroscopy for Accurate Nanoparticle Size Determination},
  volume = {16},
  issn = {1463-9084},
  doi = {10.1039/C4CP01197J},
  abstract = {We report on an experimental procedure in confocal single molecule fluorescence lifetime correlation spectroscopy (FLCS) to determine the range of excitation power and molecular or particulate concentration in solution under which the application of an unmodified model autocorrelation function is justified. This procedure enables fitting of the autocorrelation to an accurate model to measure diffusion length (r) and diffusion time ({$\tau$}D) of single molecules in solution. We also report on the pinhole size dependency of r and {$\tau$}D in a confocal FLCS platform. This procedure determines a set of experimental parameters with which the Stokes\textendash{}Einstein (S\textendash{}E) equation accurately measures the hydrodynamic radii of spherical nanoparticles, enabling the determination of the particle size range for which the hydrodynamic radius by the S\textendash{}E equation measures the real particle radius.},
